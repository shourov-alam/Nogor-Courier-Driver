<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sh.nogorcourierdriver">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/nogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.Deliveded" />
        <activity android:name=".Activity.Undelivered" />
        <activity android:name=".Activity.Driver_Profile" />
        <activity android:name=".Activity.Post_Unread" />
        <activity android:name=".Activity.Post_Read" />
        <activity android:name=".Activity.VerifyOtp" />
        <activity android:name=".Activity.Home" />
        <activity android:name=".Activity.Login" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>